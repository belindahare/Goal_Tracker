      #page-wrapper
        #page-inner
          .row
            .col-md-12
              %h2 Forms Page
              %h5 Welcome Jhon Deo , Love to see you back.
          / /. ROW
          %hr/
          .row
            .col-md-12
              / Form Elements
              .panel.panel-default
                .panel-heading
                  Form Element Examples
                .panel-body
                  .row
                    .col-md-6
                      %h3 Basic Form Examples
                      %form{:role => "form"}
                        .form-group
                          %label Text Input
                          %input.form-control/
                          %p.help-block Help text here.
                        .form-group
                          %label Text Input with Placeholder
                          %input.form-control{:placeholder => "PLease Enter Keyword"}/
                        .form-group
                          %label Just A Label Control
                          %p.form-control-static info@yourdomain.com
                        .form-group
                          %label File input
                          %input{:type => "file"}/
                        .form-group
                          %label Text area
                          %textarea.form-control{:rows => "3"}
                        .form-group
                          %label Checkboxes
                          .checkbox
                            %label
                              %input{:type => "checkbox", :value => ""}>/
                              Checkbox Example One
                          .checkbox
                            %label
                              %input{:type => "checkbox", :value => ""}>/
                              Checkbox Example Two
                          .checkbox
                            %label
                              %input{:type => "checkbox", :value => ""}>/
                              Checkbox Example Three
                        .form-group
                          %label Inline Checkboxes Examples
                          %label.checkbox-inline
                            %input{:type => "checkbox"}/
                            One
                          %label.checkbox-inline
                            %input{:type => "checkbox"}/
                            Two
                          %label.checkbox-inline
                            %input{:type => "checkbox"}/
                            Three
                        .form-group
                          %label Radio Button Examples
                          .radio
                            %label
                              %input#optionsRadios1{:checked => "checked", :name => "optionsRadios", :type => "radio", :value => "option1"}>/
                              Radio Example One
                          .radio
                            %label
                              %input#optionsRadios2{:name => "optionsRadios", :type => "radio", :value => "option2"}>/
                              Radio Example Two
                          .radio
                            %label
                              %input#optionsRadios3{:name => "optionsRadios", :type => "radio", :value => "option3"}>/
                              Radio Example Three
                        .form-group
                          %label Select Example
                          %select.form-control
                            %option One Vale
                            %option Two Vale
                            %option Three Vale
                            %option Four Vale
                        .form-group
                          %label Multiple Select Example
                          %select.form-control{:multiple => "multiple"}
                            %option One Vale
                            %option Two Vale
                            %option Three Vale
                            %option Four Vale
                        %button.btn.btn-default{:type => "submit"} Submit Button
                        %button.btn.btn-primary{:type => "reset"} Reset Button
                      %br/
                      %h3 With radio & checkboxes
                      %form
                        .form-group.input-group
                          %span.input-group-addon
                            %input{:type => "checkbox"}/
                          %input.form-control{:type => "text"}/
                        .form-group.input-group
                          %span.input-group-addon
                            %input{:type => "radio"}/
                          %input.form-control{:type => "text"}/
                    .col-md-6
                      %h3 Disabled Form State Examples
                      %form{:role => "form"}
                        %fieldset{:disabled => "disabled"}
                          .form-group
                            %label{:for => "disabledSelect"} Disabled input
                            %input#disabledInput.form-control{:disabled => "disabled", :placeholder => "Disabled input", :type => "text"}/
                          .form-group
                            %label{:for => "disabledSelect"} Disabled select
                            %select#disabledSelect.form-control
                              %option Disabled select
                          .checkbox
                            %label
                              %input{:type => "checkbox"}>/
                              Disabled Checkbox
                          %button.btn.btn-primary{:type => "submit"} Disabled Button
                      %h3 Validation State Examples
                      %form{:role => "form"}
                        .form-group.has-success
                          %label.control-label{:for => "inputSuccess"} Input with success
                          %input#inputSuccess.form-control{:type => "text"}/
                        .form-group.has-warning
                          %label.control-label{:for => "inputWarning"} Input with warning
                          %input#inputWarning.form-control{:type => "text"}/
                        .form-group.has-error
                          %label.control-label{:for => "inputError"} Input with error
                          %input#inputError.form-control{:type => "text"}/
                      %h3 Input Group Examples
                      %form{:role => "form"}
                        .form-group.input-group
                          %span.input-group-addon @
                          %input.form-control{:placeholder => "Username", :type => "text"}/
                        .form-group.input-group
                          %input.form-control{:type => "text"}/
                          %span.input-group-addon .00
                        .form-group.input-group
                          %span.input-group-addon
                            %i.fa.fa-eur
                          %input.form-control{:placeholder => "Font Awesome Icon", :type => "text"}/
                        .form-group.input-group
                          %span.input-group-addon $
                          %input.form-control{:type => "text"}/
                          %span.input-group-addon .00
                        .form-group.input-group
                          %input.form-control{:type => "text"}/
                          %span.input-group-btn
                            %button.btn.btn-default{:type => "button"}
                              %i.fa.fa-search
                      %h3 Different Size Input Groups
                      %form{:role => "form"}
                        .form-group.input-group.input-group-lg
                          %span.input-group-addon @
                          %input.form-control{:placeholder => "Username", :type => "text"}/
                        .form-group.input-group
                          %span.input-group-addon @
                          %input.form-control{:placeholder => "Username", :type => "text"}/
                        .form-group.input-group.input-group-sm
                          %span.input-group-addon @
                          %input.form-control{:placeholder => "Username", :type => "text"}/
                      %h3 Different Size Input Groups
                      %form{:role => "form"}
                        .input-group
                          %span.form-group.input-group-btn
                            %button.btn.btn-default{:type => "button"} Go!
                          %input.form-control{:type => "text"}/
                        %br/
                        .input-group
                          %input.form-control{:type => "text"}/
                          %span.form-group.input-group-btn
                            %button.btn.btn-default{:type => "button"} Go!
              / End Form Elements
          / /. ROW
          .row
            .col-md-12
